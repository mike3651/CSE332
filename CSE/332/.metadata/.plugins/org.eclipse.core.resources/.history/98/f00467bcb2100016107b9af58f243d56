import javax.swing.SwingUtilities;
import javax.swing.UIManager;

/**
 * This class kicks off the whole program.
 * 
 * @author Michael Wilson
 * @version 1.0
 */
public class Start {
	/**
	 * Private constructor that prevents the compiler 
	 * from making an unnecessary object. 
	 */
	private Start() { }
	
	/**
	 * This is the main method that kicks off the program.
	 * 
	 * @param theArgs Arguments from the command line.
	 */
	public static void main(final String[] theArgs) {
		SwingUtilities.invokeLater(new Runnable() {

			@Override
			public void run() {
				new AFrame();
			}
			
		});

	}
}
